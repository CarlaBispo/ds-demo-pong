{
  "name": "deepstream-pong",
  "version": "1.0.0",
  "description": "deepstream.io Pong",
  "scripts": {
    "deepstream": "deepstream start",
    "start": "budo src/index.js --live --serve dist/bundle.js -- -t babelify -t envify | opnr | garnish",
    "build": "browserify src/index.js -t babelify -t envify | uglifyjs > dist/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gburnett/javascript-pong.git"
  },
  "keywords": [
    "pong",
    "game"
  ],
  "author": "gburnett",
  "bugs": {
    "url": "https://github.com/gburnett/javascript-pong/issues"
  },
  "homepage": "https://github.com/gburnett/javascript-pong#readme",
  "private": true,
  "devDependencies": {
    "babelify": "^7.3.0",
    "budo": "^10.0.3",
    "envify": "^4.0.0",
    "garnish": "^5.2.0",
    "opnr": "^1.0.4",
    "uglifyjs": "^2.4.10"
  },
  "licence": "MIT",
  "dependencies": {
    "deepstream.io": "^2.2.1",
    "deepstream.io-client-js": "^2.1.4",
    "once-event-listener": "^1.0.0",
    "onetime": "^2.0.1",
    "qrcodejs2": "0.0.2"
  }
